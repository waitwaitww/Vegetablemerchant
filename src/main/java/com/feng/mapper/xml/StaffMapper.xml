<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.feng.mapper.StaffMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.feng.pojo.Staff">
        <id column="staff_id" property="staffId" />
        <result column="work_type_id" property="workTypeId" />
        <result column="staff_pwd" property="staffPwd" />
        <result column="order_quantity" property="orderQuantity" />
        <result column="base_pay" property="basePay" />
        <result column="complain" property="complain" />
        <result column="employment_date" property="employmentDate" />
        <result column="waiter_name" property="waiterName" />
        <result column="sex" property="sex" />
        <result column="staff_telephone" property="staffTelephone" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        staff_id, work_type_id, staff_pwd, order_quantity, base_pay, complain, employment_date, waiter_name, sex, staff_telephone
    </sql>

</mapper>
